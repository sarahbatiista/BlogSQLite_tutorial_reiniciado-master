<table>
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Nome</th>
      <th scope="col">Email</th>
      <th scope="col">Celular</th>
    </tr>
  </thead>
  <tbody>
    <% for(let index = 0; index < 9; index++) { %> <% if (index % 2 == 0) { %>

    <tr>
      <td><%= index %></td>
      <td>nome</td>
      <td>email</td>
      <td>celular</td>
    </tr>
    <% } %> <% } %>
  </tbody>
</table>
